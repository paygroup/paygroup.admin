{
    "files.autoSave": "afterDelay",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "files.exclude": {
      "node_modules": false
    },
    "eslint.options": {
      "extensions": [".js", ".jsx", ".md", ".mdx", ".ts", ".tsx"]
    },
    "eslint.validate": [
      "markdown",
      "mdx",
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ]
  }
  